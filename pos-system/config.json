{
  "name": "POS System",
  "version": "2.0.27",
  "slug": "pos_system",
  "description": "Point of Sale System with Barcode Scanner support",
  "url": "https://github.com/skynetwifi/homeassistant-addon-pos",
  "arch": ["amd64", "aarch64", "armv7", "armhf", "i386"],
  "startup": "services",
  "boot": "auto",
  "init": false,
  "ingress": true,
  "ingress_port": 8099,
  "panel_icon": "mdi:cash-register",
  "panel_title": "POS",
  "homeassistant_api": true,
  "auth_api": true,
  "options": {
    "db_host": "core-mariadb",
    "db_port": 3306,
    "db_name": "pos_db",
    "db_user": "pos_user",
    "db_password": "password",
    "admin_user": "admin",
    "admin_password": "admin123"
  },
  "schema": {
    "db_host": "str",
    "db_port": "int",
    "db_name": "str",
    "db_user": "str",
    "db_password": "password",
    "admin_user": "str",
    "admin_password": "password"
  },
  "ports": {
    "8099/tcp": null,
    "8100/tcp": null
  },
  "ports_description": {
    "8099/tcp": "Ingress (Internal)",
    "8100/tcp": "Secure HTTPS"
  },
  "map": ["share:rw", "ssl"],
  "environment": {
    "NODE_ENV": "production"
  }
}
